            foreach(var item in dic)
            {
                if (item.Key == 1)
                {
                    foreach (string s in item.Value)
                    {
                        row = objDT.NewRow();
                        row["HashID"] = 1;
                        row["FileAllPath"] = s;
                        objDT.Rows.Add(row);
                        // MessageBox.Show(item.Key + " " + s);
                    }
                }

                if (item.Key == 2)
                {

                    foreach (string s in item.Value)
                    {
                        row = objDT.NewRow();
                        row["HashID"] = 2;
                        row["FileAllPath"] = s;
                        objDT.Rows.Add(row);
                        // MessageBox.Show(item.Key + " " + s);
                    }
                }

                if (item.Key == 3)
                {
                    foreach (string s in item.Value)
                    {
                        row = objDT.NewRow();
                        row["HashID"] = 3;
                        row["FileAllPath"] = s;
                        objDT.Rows.Add(row);
                        // MessageBox.Show(item.Key + " " + s);
                    }
                }

                /*foreach (string s in item.Value)
                {
                    MessageBox.Show(item.Key+" "+ s);
                }*/
                
            }
            DataSet ds = new DataSet();
