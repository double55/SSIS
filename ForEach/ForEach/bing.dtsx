        public void Main()
		{
            // TODO: Add your code here
            // User::FilePath0,User::FilePath1,User::FilePath2,User::Num0,User::Num1,User::Num2
            // FileInfo FilePath1 = new FileInfo(Dts.Variables["localFile"].Value.ToString());

            string[] localFiles;

            string DirPath = Dts.Variables["DirPath"].Value.ToString();

            Dictionary<int, List<string>> dic = new Dictionary<int, List<string>> { };
            //声明一个链表
            List<string> list0 = new List<string>();
            List<string> list1 = new List<string>();
            List<string> list2 = new List<string>();
            int i = 1;
            localFiles = Directory.GetFiles(DirPath, FILE_FILTER);
            int a = 0;
            foreach (string localFile in localFiles)
            {
                a = i % 3;
                MessageBox.Show(localFile);
                // MessageBox.Show(a + " " + i);
                if (a == 0)
                {
                    list0.Add(localFile);
                    //MessageBox.Show("a==0"+list0);
                }
                if (a == 1)
                {
                    list1.Add(localFile);
                    //MessageBox.Show("a==1" + list1);
                }
                if (a == 2)
                {
                    list2.Add(localFile);
                    //MessageBox.Show("a==2" + list2);
                }
                i += 1;

            }
            Dts.Variables["User::Num0"].Value = list0.Count;
            Dts.Variables["User::FilePath0"].Value = list0;

            Dts.Variables["User::Num1"].Value = list1.Count;
            Dts.Variables["User::FilePath1"].Value = list1;

            Dts.Variables["User::Num2"].Value = list2.Count;
            Dts.Variables["User::FilePath2"].Value = list2;

            Dts.TaskResult = (int)ScriptResults.Success;
		}

