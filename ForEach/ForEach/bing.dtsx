            OleDbDataAdapter objDA = new OleDbDataAdapter();

            // Create a new DataTable.
            
            DataTable objDT = new DataTable("FileHashTable");
            
            DataColumn column;
            DataRow row;

            // Create first column and add to the DataTable.
            column = new DataColumn();
            {
                column.DataType = System.Type.GetType("System.Int32");
                column.ColumnName = "HashID";
                column.AutoIncrement = false;
                column.Caption = "ID";
                column.ReadOnly = false;
                column.Unique = false;
            }
            // Add the column to the DataColumnCollection.
            objDT.Columns.Add(column);

            // Create second column.
            column = new DataColumn();
            {
                // objDT.Columns.Clear();
                column.DataType = System.Type.GetType("System.String");
                column.ColumnName = "FileAllPath";
                column.AutoIncrement = false;
                column.Caption = "FileItem";
                column.ReadOnly = false;
                column.Unique = false;
                objDT.Columns.Add(column);
            }
            
            

            // objDA.Fill(objDT, Dts.Variables["User::FilePath0"].Value);
            /*foreach(DataRow dr in objDT.Rows)
