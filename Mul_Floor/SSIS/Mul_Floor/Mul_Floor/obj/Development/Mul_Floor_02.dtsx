        public void Main()
		{
            // TODO: Add your code here

            // User::varDataSourceDirPath,User::varFileExtension

            // User::varDirFirst,User::varDirFirstPath,User::varDirSecs,User::varFilePaths


            string DataSourceDirPath = Dts.Variables["User::varDataSourceDirPath"].Value.ToString();
            string FileExtension = Dts.Variables["User::varFileExtension"].Value.ToString();
            string DirFirst = Dts.Variables["User::varDirFirst"].Value.ToString();

            string path = DataSourceDirPath + "\\" + DirFirst;
            // ArrayList al = new ArrayList();

            Dts.Variables["User::varDirFirstPath"].Value = path;

            // MessageBox.Show(path);

            List<string> DirSecs = new List<string>();
            List<string> FilePath = new List<string>();
            List<string> FilePathObj = new List<string>();
        
            DirectoryInfo root = new DirectoryInfo(path);
            DirectoryInfo[] dics = root.GetDirectories();
            
            GetAllDirList(path, FilePath, FilePathObj);
            // MessageBox.Show(path);
            foreach (var item in dics)
            {
                // MessageBox.Show(item.ToString());
                DirSecs.Add(item.ToString());
            }
            Dts.Variables["User::varDirSecs"].Value = DirSecs;



            Dts.TaskResult = (int)ScriptResults.Success;
        }

        // 递归查询文件


        //我把List<string>当成动态数组用，非常好用
        public void GetAllDirList(string strBaseDir, List<string> FilePath, List<string> FilePathObj)
        {
            DirectoryInfo di = new DirectoryInfo(strBaseDir);
            DirectoryInfo[] diA = di.GetDirectories();
            
            if(diA.Length>0)
            {
                for (int i = 0; i < diA.Length; i++)

                {
                    string path = diA[i].FullName;
                    // MessageBox.Show(path);
                    
                    //diA[i].FullName是某个子目录的绝对地址，把它记录在ArrayList中
                    GetAllDirList(path, FilePath, FilePathObj);
                    //注意：递归了。逻辑思维正常的人应该能反应过来
                }
            }
            else
            { 
                FilePath.Add(strBaseDir);
              
                // MessageBox.Show("strBaseDir:" + strBaseDir);
                string[] localFiles = Directory.GetFiles(strBaseDir, Dts.Variables["User::varFileExtension"].Value.ToString());
                // MessageBox.Show("localFile:"+localFiles.Length);
                foreach (string localFile in localFiles)
                {
                    
                    FilePathObj.Add(localFile);
                    // MessageBox.Show("localFile:" + localFile);
                }
                Dts.Variables["User::varFilePaths"].Value = FilePathObj;
                // MessageBox.Show(strBaseDir);
                return;
            }

            // MessageBox.Show(strBaseDir);


        }
